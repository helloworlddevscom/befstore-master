    "description": "local wordpress dev env",
    "authors": [
        {
            "name"    : "Jeff Browning"
        }
    ],
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url" : "https://wpackagist.org"
        }
    ],
    "config": {
